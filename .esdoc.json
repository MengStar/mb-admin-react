{
    "source": "./app",
    "destination": "./docs",
    "includes": ["\\.js$"],
    "excludes": ["bundle\\.js$", "main\\.js$"],
    "plugins": [
        {"name": "esdoc-jsx-plugin", "option": {"enable": true}},
        {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
        {"name": "esdoc-standard-plugin", "option": {"lint": {"enable": false}}},
        {"name": "esdoc-inject-style-plugin", "option": {"enable": false, "styles": ["./foo.css"]}}
    ]
}
